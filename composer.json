{
    "name": "ampeco/omnipay-fibank",
    "type": "module",
    "description": "Omnipay plugin for fibank ECOMM recurring payments",
    "version": "1.0.0",
    "keywords": [
        "omnipay",
        "fibank",
        "ecomm",
        "recurring payments"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "omnipay/common": "^3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ampeco\\OmnipayFibank\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
